local function eatBoosts()
    local player = game.Players.LocalPlayer
    local backpack = player.Backpack
    local boosts = {"ULTRA Shake"} -- Boosts that you want to eat
    
    for _, boostName in ipairs(boosts) do
        local boost = backpack:FindFirstChild(boostName)
        while boost do
            -- Ensure the player's character is valid
            if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
                -- Equip the boost item
                boost.Parent = player.Character
                
                -- Check if the boost is a Tool and has an Activate method
                if boost:IsA("Tool") then
                    -- Assuming Activate is a method you want to call
                    if boost:FindFirstChild("Activate") and type(boost.Activate) == "function" then
                        -- Consume the boost item
                        boost:Activate()
                    else
                        print(boostName .. " does not have an 'Activate' method or is not a Tool.")
                    end
                    
                    -- Return the tool back to the backpack if needed
                    boost.Parent = backpack
                else
                    print(boostName .. " is not a Tool.")
                end
            else
                print("Player's character is not valid.")
                break -- Exit the loop if the character is invalid
            end
            
            -- Check for the next instance of the boost in the backpack
            boost = backpack:FindFirstChild(boostName)
        end
    end
end

local function createEatBoostsGui()
    local player = game.Players.LocalPlayer
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "EatBoosts"
    screenGui.Parent = player.PlayerGui

    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 200, 0, 50)
    button.Position = UDim2.new(0.5, -100, 0.5, -25)
    button.Text = "Eat All Boosts"
    button.Parent = screenGui

    button.MouseButton1Click:Connect(eatBoosts)
end

createEatBoostsGui()
